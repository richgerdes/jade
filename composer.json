{
    "name": "trivago/jade",
    "description": "A simple but yet powerful library to expose your entities through JSON API.",
    "license": "Apache-2.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Trivago\\Jade\\": "Trivago/Jade"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "neomerx/json-api": "^1.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/doctrine-bridge": "^3.0|^4.0",
        "symfony/security-bundle": "^3.0|^4.0",
        "symfony/framework-bundle": "^3.0|^4.0"
    },
    "authors": [
        {
            "name": "trivago",
            "homepage": "https://github.com/trivago/jade/graphs/contributors"
        }
    ],
    "require-dev": {
        "webmozart/assert": "^1.4",
        "codeception/codeception": "^2.3",
        "fzaninotto/faker": "^1.7",
        "phpspec/prophecy": "^1.7",
        "phpunit/phpunit": "^5.7",
        "php-di/phpdoc-reader": "^2.0"
    }
}
