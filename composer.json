{
    "name": "trivago/jade",
    "description": "A simple but yet powerful library to expose your entities through JSON API.",
    "license": "Apache-2.0",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Trivago\\Jade\\": "src"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "neomerx/json-api": "^4.0",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^2.0",
        "symfony/doctrine-bridge": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/framework-bundle": "^6.0"
    },
    "authors": [
        {
            "name": "trivago",
            "homepage": "https://github.com/trivago/jade/graphs/contributors"
        }
    ],
    "require-dev": {
        "webmozart/assert": "^1.4",
        "codeception/codeception": "^5.0",
        "phpunit/phpunit": "^10.4",
        "php-di/phpdoc-reader": "^2.2"
    }
}
